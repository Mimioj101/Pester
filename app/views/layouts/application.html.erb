<!DOCTYPE html>
<html>
  <head>
    <title>Pester</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

  
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">
    <%= stylesheet_link_tag 'application', "cockroaches", media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand" href="#">PESTER</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav">

      <% if @current_user%>
          <li class="nav-item active">
            <a class="nav-link" href="/users/<%=@current_user.id%>">Profile Page <span class="sr-only">(current)</span></a>
          </li>
  
      <li class="nav-item">
        <a class="nav-link" href="/cockroaches/">Cockroaches</a>
      </li>

            <li class="nav-item">
        <%=link_to "Logout", logout_path, method: :delete, class: "nav-link" %>
      </li>

      <%else%>

            <li class="nav-item">
         <%=link_to "Login", new_login_path, method: :get, class: "nav-link" %>
      </li>

            <li class="nav-item">
        <a class="nav-link" href="/users/new">Signup</a>
      </li>

  <%end%>
    </ul>
  </div>
</nav>
    <%= yield %>


  </body>
</html>